        "symfony",
        "base"
    ],
    "homepage": "https://github.com/VincentChalnot/SidusBaseBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Chalnot",
            "email": "vincent@sidus.fr",
            "homepage": "http://chalnot.fr",
